   "source": [
    "def gcf(a,b):\n",
    "    if a>b:\n",
    "        return gcf(b,a)\n",
    "    while b%a != 0 :\n",
    "        a,b = b%a, a\n",
    "    return a\n",
    "def lcf(a,b):\n",
    "    return a*b/gcf(a,b)   #definition of lcf\n",
    "\n",
    "for range(1,21):\n",
    "    n= lcf(n,i)"
   ]
